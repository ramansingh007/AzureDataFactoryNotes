{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-prabjyot"
		},
		"AzureSqlDB_Datasets_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDB_Datasets'"
		},
		"AzureSqlDatabaseForLogging_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseForLogging'"
		},
		"Copydata_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Copydata'"
		},
		"copy data_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'copy data'"
		},
		"sourceMetaData_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sourceMetaData'"
		},
		"AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://Prabjyot.vault.azure.net/"
		},
		"Copydata_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://prabjyotstorage.dfs.core.windows.net"
		},
		"SourceDatasets_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://prabjyotstorage.dfs.core.windows.net"
		},
		"copy data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://prabjyotstorage.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-prabjyot_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDB_Datasets_connectionString": {
						"value": "[parameters('AzureSqlDB_Datasets_connectionString')]"
					},
					"AzureSqlDatabaseForLogging_connectionString": {
						"value": "[parameters('AzureSqlDatabaseForLogging_connectionString')]"
					},
					"Copydata_accountKey": {
						"value": "[parameters('Copydata_accountKey')]"
					},
					"copy data_accountKey": {
						"value": "[parameters('copy data_accountKey')]"
					},
					"sourceMetaData_connectionString": {
						"value": "[parameters('sourceMetaData_connectionString')]"
					},
					"AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"Copydata_properties_typeProperties_url": {
						"value": "[parameters('Copydata_properties_typeProperties_url')]"
					},
					"copy data_properties_typeProperties_url": {
						"value": "[parameters('copy data_properties_typeProperties_url')]"
					},
					"SourceDatasets_properties_typeProperties_url": {
						"value": "[parameters('SourceDatasets_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}